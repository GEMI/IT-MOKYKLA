  <body>
    <pre>* {
  box-sizing: border-box;
}

body {
  font-family: Arial, Helvetica, sans-serif;
	background-image: url("doggy.jpg");
	background-size: cover;
}
header {
  padding: 10px;
  text-align: center;
  font-size: 22px;
  color: white;
	height: 100px;
	background-color:hsla(30, 59%, 76%, 0.2);
}

nav {
  float: left;
  width: 10%;
  height: 800px; 
  padding: 0;
	background-color:hsla(30, 59%, 76%, 0.2);

}

ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
	background-color:hsla(30, 59%, 76%, 0.2);
}

li a {
  display: block;
  color: #000;
  padding: 18px 16px;
  text-decoration: none;
	text-align:center;
}

li a:hover {
  background-color: #555;
  color: white;
}

article {
  float: left;
  padding: 20px;
  width: 90%;
	height: 800px;
	}
footer {
  background-color:hsla(30, 59%, 76%, 0.2);
  padding: 20px;
  text-align: center;
  color: white;
	float:left;
width:100%;<br>}<br>.selectpicker {<br>  position: relative;
